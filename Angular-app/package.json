{
  "name": "h-budget",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --watch --open",
    "build": "ng build",
    "build-prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test-headless-ci-only": "ng test --browsers ChromiumNoSandbox",
    "lint": "ng lint --fix",
    "lint-css": "stylelint src/**/*.css",
    "prettier": "prettier --write src/**/*.{js,ts,css}",
    "eslint": "eslint --cache --fix --ext .js,.jsx,.ts,.tsx src",
    "eslint:dump": "eslint --print-config ./.eslintrc.json",
    "sonar-scanner": "sonar-scanner",
    "test-headless": "ng test --watch=false --code-coverage --source-map=false --browsers ChromiumNoSandbox"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged && ng lint --fix && ng test --watch=false --browsers=ChromeHeadless",
      "pre-push": "ng build --aot true && ng lint && ng test --browsers ChromiumNoSandbox"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.1",
    "@angular/cdk": "^14.0.1",
    "@angular/common": "^14.0.1",
    "@angular/compiler": "^14.0.1",
    "@angular/core": "^14.0.1",
    "@angular/forms": "^14.0.1",
    "@angular/material": "^14.0.1",
    "@angular/platform-browser": "^14.0.1",
    "@angular/platform-browser-dynamic": "^14.0.1",
    "@angular/router": "^14.0.1",
    "@ngxs/logger-plugin": "^3.7.4",
    "@ngxs/store": "^3.7.4",
    "apexcharts": "^3.35.3",
    "codelyzer": "^6.0.2",
    "date-fns": "^2.28.0",
    "lodash": "^4.17.21",
    "material-design-icons": "^3.0.1",
    "ng-apexcharts": "^1.7.1",
    "rxjs": "~7.5.5",
    "stylelint": "^14.9.1",
    "tslib": "^2.4.0",
    "tslint": "^6.1.3",
    "typescript-guid": "^1.0.3",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.1",
    "@angular-eslint/builder": "14.0.0-alpha.3",
    "@angular-eslint/eslint-plugin": "14.0.0-alpha.3",
    "@angular-eslint/eslint-plugin-template": "14.0.0-alpha.3",
    "@angular-eslint/schematics": "14.0.0-alpha.3",
    "@angular-eslint/template-parser": "14.0.0-alpha.3",
    "@angular/cli": "^14.0.1",
    "@angular/compiler-cli": "^14.0.1",
    "@ngxs/devtools-plugin": "^3.7.4",
    "@types/jasmine": "~4.0.3",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.43",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jasmine-core": "~4.2.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.1",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-sonarqube-reporter": "^1.4.0",
    "lint-staged": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.0",
    "pretty-quick": "^3.1.3",
    "sonarqube-scanner": "^2.8.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-prettier": "^2.0.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "~4.7.3"
  }
}